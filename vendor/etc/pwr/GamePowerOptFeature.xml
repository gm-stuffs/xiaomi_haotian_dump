        <FREQ_MIN_Enable>0, 0</FREQ_MIN_Enable>
        <SCALING_MAX_FREQ>0x40804000, 0x40804200</SCALING_MAX_FREQ>
        <SCALING_MIN_FREQ>0x40800000, 0x40800200</SCALING_MIN_FREQ>
